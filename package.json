{
  "name": "response-iterator",
  "version": "0.2.6",
  "description": "Creates an async iterator for a variety of inputs in the browser and node. Supports fetch, node-fetch, and cross-fetch",
  "keywords": [
    "Symbol",
    "Symbol.asyncIterator",
    "array",
    "arrayBuffer",
    "asyncIterator",
    "axios",
    "blob",
    "body",
    "browser",
    "cross-fetch",
    "fetch",
    "got",
    "isomorphic-fetch",
    "iteration",
    "iterator",
    "node",
    "node-fetch",
    "response",
    "stream",
    "typescript",
    "undici"
  ],
  "homepage": "https://github.com/kmalakoff/response-iterator#README.md",
  "bugs": {
    "url": "https://github.com/kmalakoff/response-iterator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/response-iterator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kevin Malakoff",
    "email": "kmalakoff@gmail.com",
    "url": "https://github.com/kmalakoff"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/response-iterator.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply src/ test/",
    "test": "tsds test:node && tsds test:node \"test/**/*.test-node.*\" && tsds test:browser",
    "test:engines": "nvu engines tsds test:node && tsds test:browser"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/axios": "^0.14.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.17",
    "@types/node-fetch": "^3.0.3",
    "cross-fetch": "^4.0.0",
    "got": "^14.2.0",
    "isomorphic-fetch": "^3.0.0",
    "readable-stream-2": "npm:readable-stream@2.3.7",
    "readable-stream-3": "npm:readable-stream@4.5.2",
    "ts-dev-stack": "^0.9.3",
    "undici": "^6.6.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
